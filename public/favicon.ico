<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Dégradé principal -->
    <linearGradient id="bg" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#4f46e5"/>
      <stop offset="100%" stop-color="#1e3a8a"/>
    </linearGradient>

    <!-- Ombre douce -->
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="0" dy="12" stdDeviation="18" flood-color="#000" flood-opacity="0.25"/>
    </filter>
  </defs>

  <!-- Fond arrondi -->
  <rect width="512" height="512" rx="120" fill="url(#bg)"/>

  <!-- Carte calendrier -->
  <rect x="96" y="130" width="320" height="270" rx="50" fill="white" filter="url(#shadow)"/>

  <!-- Barre supérieure -->
  <rect x="96" y="130" width="320" height="80" rx="50" fill="#e0e7ff"/>

  <!-- Anneaux -->
  <rect x="150" y="90" width="28" height="70" rx="10" fill="white"/>
  <rect x="334" y="90" width="28" height="70" rx="10" fill="white"/>

  <!-- Ligne horizontale -->
  <line x1="120" y1="230" x2="392" y2="230" stroke="#c7d2fe" stroke-width="6"/>

  <!-- Points d’événements -->
  <circle cx="170" cy="270" r="14" fill="#4f46e5"/>
  <circle cx="256" cy="270" r="14" fill="#22c55e"/>
  <circle cx="342" cy="270" r="14" fill="#f59e0b"/>

  <circle cx="170" cy="330" r="14" fill="#ef4444"/>
  <circle cx="256" cy="330" r="14" fill="#4f46e5"/>
  <circle cx="342" cy="330" r="14" fill="#22c55e"/>
</svg>
